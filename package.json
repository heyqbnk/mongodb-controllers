{
  "name": "@kibcode/mongodb-controllers",
  "version": "1.1.2",
  "private": false,
  "description": "Collection controllers for MongoDB",
  "repository": "git@github.com:wolframdeus/mongodb-controllers.git",
  "homepage": "https://github.com/wolframdeus/mongodb-controllers",
  "bugs": "https://github.com/wolframdeus/mongodb-controllers/issues",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Vladislav Kibenko <wolfram.deus@gmail.com>",
  "license": "MIT",
  "keywords": [
    "mongodb",
    "controllers"
  ],
  "scripts": {
    "build": "gulp",
    "commit": "git-cz",
    "test": "tsc --noEmit",
    "release": "standard-version -a",
    "postrelease": "npm-run-all release:*",
    "release:tags": "git push --follow-tags origin master && yarn publish --non-interactive --access public",
    "release:github": "dotenv -e .env -- conventional-github-releaser -p eslint",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.1.2",
    "conventional-github-releaser": "^3.1.3",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.0.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-minify": "^3.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0",
    "ts-loader": "^8.0.9",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "mongodb": "^3.6.3"
  }
}
